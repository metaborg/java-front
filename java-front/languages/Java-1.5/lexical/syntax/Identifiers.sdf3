%%%
 %% Section 3.8: Identifiers
 %%%
module Identifiers

imports 

  Keywords

sorts

  Id ID

context-free syntax

  Id.Id = ID 

lexical syntax

  ID = [A-Za-z\_\$] [A-Za-z0-9\_\$]* 
  ID = Keyword {reject}
  ID = "true" {reject}
  ID = "false" {reject}
  ID = "null" {reject}

lexical restrictions

  ID -/- [a-zA-Z0-9\_\$]
