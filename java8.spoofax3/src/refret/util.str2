module refret/util

imports

  strategolib
  libstratego-aterm

  statix/api

  signatures/syntax/java/lexical/-

rules

  foldl'(a -> b, a * b -> b|) :: List(a) -> b
  foldl'(s1, s2) = ?[h| tl]; !(tl, <s1> h); foldl(s2)

rules

  dbg :: TP
  dbg(? -> string) :: TP

  dbg       = where((pp-aterm <+ strip-annos); debug)
  dbg(str)  = where((pp-aterm <+ strip-annos); debug(str))
