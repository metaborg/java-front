module moveclass/intellij/moveclass/unusedImport

test unusedImport: move class test [[
  test;
  [p2] {
  [F2]
      package p2;
  
    public class [[F2]] {}
  }
  [p1] {
  [F1]
      package [[p1]];
  
    import p2.F2;
  
    class F1 {
      F2 f2;
    }
  }
]] run move-class(|#1, #2) to [[
  test;
  [p1] {
  [F1]
      package p1;
  
    class F1 {
      F2 f2;
    }
  [F2]
      package p1;
  
    public class F2 {}
  }
]]

