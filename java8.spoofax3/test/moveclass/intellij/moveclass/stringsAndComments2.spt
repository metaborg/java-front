module moveclass/intellij/moveclass/stringsAndComments2

test stringsAndComments2: move class test succeeds [[
  test;
  [pack1] {
  [AClass]
    package pack1;
  
    public class [[AClass]] {
      String s = "pack1.AClass";
    }
  
  }
  [pack2] {
  [AClass]
    package [[pack2]];
  }
]] run move-class(|#1, #2)

test stringsAndComments2: move class test [[
  test;
  [pack1] {
  [AClass]
    package pack1;
  
    public class [[AClass]] {
      String s = "pack1.AClass";
    }
  
  }
  [pack2] {
  [AClass]
    package [[pack2]];
  }
]] run move-class(|#1, #2) to [[
  test;
  [pack2] {
  [AClass]
    package pack2;
  
    public class AClass {
      // NOTE: String are not changed in our implementation
      String s = "pack1.AClass";
    }
  }
]]

