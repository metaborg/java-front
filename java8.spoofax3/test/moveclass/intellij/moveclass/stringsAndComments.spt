test stringsAndComments: move class test succeeds [[
  test;
  [Client]
    public class Client {
      String s = "pack1.Class1";
      // pack1.Class1
      // apack1.Class1
      // Class1
    }
  [pack1] {
  [Class1]
    package pack1;
  
    public class [[Class1]] { }
  }
  [pack2] {
  [Class1]
    package [[pack2]];
  
  }
]] run move-class(|#1, #2)

