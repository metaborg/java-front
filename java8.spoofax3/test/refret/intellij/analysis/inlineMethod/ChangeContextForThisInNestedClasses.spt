module refret/intellij/analysis/inlineMethod/ChangeContextForThisInNestedClasses

test ChangeContextForThisInNestedClasses: default analysis [[
  test;
  [Test]
      abstract class Test {
  
        void foo() {
      }
    }
    class Test2 extends Test {
  
      {
          new Object() {
            void foo() {
              Test2.this.foo();
            }
          };
      }
    }
]] analysis succeeds

test ChangeContextForThisInNestedClasses: test analysis [[
  test;
  [Test]
      abstract class Test {
  
        void foo() {
      }
    }
    class Test2 extends Test {
  
      {
          new Object() {
