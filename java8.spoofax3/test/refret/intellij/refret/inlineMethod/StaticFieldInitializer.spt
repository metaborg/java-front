module refret/intellij/refret/inlineMethod/StaticFieldInitializer

test StaticFieldInitializer: refret test 1 [[
  test;
  [Test]
      class A{
        static int [[field]];
  
        static {
            doSomething();
            [[field]] = 1;
        }
  
        static void doSomething() { }
  
    }
]] run fix-reference(|#2, #1) to [[
  test;
  [Test]
      class A{
        static int field;
  
        static {
            doSomething();
            field = 1;
        }
  
        static void doSomething() { }
  
    }
]]

