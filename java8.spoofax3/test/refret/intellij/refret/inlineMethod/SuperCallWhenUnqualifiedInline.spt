module refret/intellij/refret/inlineMethod/SuperCallWhenUnqualifiedInline

test SuperCallWhenUnqualifiedInline: refret test 1 [[
  test;
  [Test]
      class A {
      void [[bar]]() {}
    }
  
    class B extends A {
      void foo() {
        super.bar();
      }
  
      void err() {
          [[super.bar()]];
      }
    }
]] run fix-reference(|#2, #1) to [[
  test;
  [Test]
      class A {
      void bar() {}
    }
  
    class B extends A {
      void foo() {
        super.bar();
      }
  
      void err() {
          super.bar();
      }
    }
]]

