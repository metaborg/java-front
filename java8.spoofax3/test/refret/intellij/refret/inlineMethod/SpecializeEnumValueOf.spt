module refret/intellij/refret/inlineMethod/SpecializeEnumValueOf

test SpecializeEnumValueOf: refret test 1 [[
  test;
  [foo] {
  [bar] {
  [baz] {
  [Test]
      package foo.bar.baz;
  
    class Test {
      enum X {A, [[B]], C};
  
      void test() {
          System.out.println([[B]]);
      }
    }
  }
  }
  }
]] run fix-reference(|#2, #1) to [[
  test;
  [foo] {
  [bar] {
  [baz] {
  [Test]
      package foo.bar.baz;
  
    class Test {
      enum X {A, B, C};
  
      void test() {
          System.out.println(X.B);
      }
    }
  }
  }
  }
]]

