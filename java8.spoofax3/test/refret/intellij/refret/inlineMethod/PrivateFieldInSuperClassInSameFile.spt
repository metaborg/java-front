module refret/intellij/refret/inlineMethod/PrivateFieldInSuperClassInSameFile

test PrivateFieldInSuperClassInSameFile: refret test 1 [[
  test;
  [Test]
      class Test {
      class A {
        private Integer [[i]] = 0;
  
      }
  
      class B extends A {
        void bar(A [[a]]) {
            [[i]].toString();
        }
      }
    }
]] run fix-reference(|#3, #1, #2) to [[
  test;
  [Test]
      class Test {
      class A {
        private Integer i = 0;
  
      }
  
      class B extends A {
        void bar(A a) {
            a.i.toString();
        }
      }
    }
]]

