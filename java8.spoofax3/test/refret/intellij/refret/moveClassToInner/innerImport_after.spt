module refret/intellij/refret/moveClassToInner/innerImport_after

test innerImport_after: refret test 1 [[
  test;
  [pack2] {
  [A]
      package pack2;
  
    public class [[A]] {
        public static class Class1 {
        }
    }
  
  }
  [packUser] {
  [Class2]
      package packUser;
  
    import [[A]];
  
    public class Class2 {
      A.Class1 myClass1;
    }
  }
]] run fix-reference(|#2, #1) to [[
  test;
  [pack2] {
  [A]
      package pack2;
  
    public class A {
        public static class Class1 {
        }
    }
  
  }
  [packUser] {
  [Class2]
      package packUser;
  
    import pack2.A;
  
    public class Class2 {
      A.Class1 myClass1;
    }
  }
]]

